.tab-pane
  .col-md-10.col-md-offset-1
    table.table.table-striped.table-bordered.table-hover
      tbody
        tr
          td
            | Service
          td
            = @user_elasticsearch_solution.class.human_name
        tr
          td
            | IP Address
          td
            = @user_elasticsearch_solution.ip_address
        tr
          td
            | Index Name
          td
            = @user_elasticsearch_solution.index_name
        tr
          td
            | Host
          td
            = @user_elasticsearch_solution.host
        tr
          td
            | Port
          td
            = @user_elasticsearch_solution.port
        tr
          td
            | External id
          td
            = @user_elasticsearch_solution.external_id
        tr
          td
            | Datasets
          td
            ul
              - @user_elasticsearch_solution.datasets.each do |dataset|
                li
                  = link_to dataset_path(dataset) do
                    = dataset.name
    = link_to 'Back', user_solutions_path
