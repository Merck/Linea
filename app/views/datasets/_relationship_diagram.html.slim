#relationship-diagram
  .row
    .col-md-6.title
      | Datasets with Matching Columns
    .col-md-6.title.right
      | Columns in this Dataset

javascript:
	var related_datasets = #{raw(@related_datasets.to_json)};
	var columns = #{raw(@columns.to_json)};
  var relations = #{raw(@relations.to_json)};

  $("#relationship_diagram_link").on("shown.bs.tab", function renderDiagram () {
    var diagram = new SharedColumnsDiagram(related_datasets, columns, relations, 700, 600);
    diagram.load();
    $(this).off("shown.bs.tab", renderDiagram);
  });
